  "scripts": {
    "prepare": "npm run build",
    "lint": "tslint -p",
    "start": "node ./dist/app.js",
    "build": "tsc -p .",
    "clean": "rm -rf ./dist",
    "deploy": "npm run clean && npm run build && gcloud app deploy --project tweq-5cbdb",
    "start:local": "npm run build && pm2 restart ecosystem.config.js --only dev",
    "dev": "set NODE_ENV=development && nodemon --watch 'src/**/*.ts' --exec ts-node src/app.ts",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec ts-node src/app.ts",
    "test": "mocha --exit **/*.test.js",
    "start:functions": "cd ./functions && yarn start",
    "serve:functions": "cd ./functions && yarn serve",
    "shell:functions": "cd ./functions && yarn shell",
    "deploy:functions": "cd ./functions && yarn deploy",
    "logs:functions": "cd ./functions && yarn logs"
  },
